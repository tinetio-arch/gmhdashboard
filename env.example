DATABASE_HOST=clinic-pg.cbkcu8m4geoo.us-east-2.rds.amazonaws.com
DATABASE_PORT=5432
DATABASE_NAME=clinicdb
DATABASE_USER=clinicadmin
DATABASE_PASSWORD=replace-me
DATABASE_SSLMODE=require
NEXT_PUBLIC_BASE_PATH=/ops

# QuickBooks Online API OAuth Credentials
# Get these from: https://developer.intuit.com/app/developer/myapps
QUICKBOOKS_CLIENT_ID=your_client_id_here
QUICKBOOKS_CLIENT_SECRET=your_client_secret_here
QUICKBOOKS_REDIRECT_URI=http://localhost:3000/api/auth/quickbooks/callback
QUICKBOOKS_REALM_ID=your_realm_id_here
QUICKBOOKS_ACCESS_TOKEN=your_access_token_here
QUICKBOOKS_REFRESH_TOKEN=your_refresh_token_here
QUICKBOOKS_ENVIRONMENT=production

# Go-High-Level API Credentials
# Get these from: https://highlevel.stoplight.io/docs/integrations
GHL_API_KEY=your_ghl_api_key_here
GHL_LOCATION_ID=your_location_id_here
GHL_BASE_URL=https://services.leadconnectorhq.com
GHL_DEBUG=false

# ClinicSync webhook reprocess logging
CLINICSYNC_DEBUG=false

# Internal cron auth
INTERNAL_AUTH_SECRET=replace-with-strong-secret

# Session / NextAuth configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace-with-nextauth-secret

# Healthie API Credentials
# Get these from: https://docs.gethealthie.com/
HEALTHIE_API_KEY=your_healthie_api_key_here
HEALTHIE_API_URL=https://api.gethealthie.com/graphql
HEALTHIE_DEBUG=false
HEALTHIE_APP_BASE_URL=https://secure.gethealthie.com
HEALTHIE_TRT_REGIMEN_META_KEY=trt_regimen
HEALTHIE_LAST_DISPENSE_META_KEY=last_dispense_date
HEALTHIE_WEBHOOK_SECRET=replace-with-strong-secret

# Heidi (clinical AI) integration
# Docs: https://www.heidihealth.com/developers/heidi-api/overview
HEIDI_API_KEY=your_heidi_api_key_here
HEIDI_API_BASE_URL=https://registrar.api.heidihealth.com/api/v2/ml-scribe/open-api/
NEXT_PUBLIC_HEIDI_WIDGET_URL=https://widget.heidihealth.com/sdk.js
HEIDI_DEFAULT_USER_EMAIL=your_default_heidi_user_email
HEIDI_DEFAULT_USER_ID=ops-dashboard
